{
    "name": "bvnsp-signin-twilio",
    "version": "1.1.0",
    "private": true,
    "scripts": {
        "build": "./node_modules/.bin/webpack",
        "prestart": "npm run build",
        "start": "twilio serverless:run",
        "debug": "npm run build && twilio serverless:run --inspect",
        "deploy": "run-s build deploy_prod_logic",
        "deploy_dev": "run-s build deploy_dev_logic",
        "deploy_dev_logic": "twilio serverless:deploy",
        "deploy_prod_logic": "twilio serverless:deploy --environment prod",
        "test": "jest",
        "tester": "ts-node tester.ts"
    },
    "dependencies": {
        "@twilio-labs/serverless-runtime-types": "^2.2.3",
        "@twilio/runtime-handler": "1.3.0",
        "googleapis": "^118.0.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.6",
        "@types/node-fetch": "^2.6.7",
        "assert": "^2.1.0",
        "buffer": "^6.0.3",
        "crypto-browserify": "^3.12.0",
        "dotenv": "^16.3.1",
        "fast-xml-parser": "^4.3.2",
        "https-browserify": "^1.0.0",
        "jest": "^29.7.0",
        "node-fetch": "^2.7.0",
        "npm-run-all": "^4.1.5",
        "os-browserify": "^0.3.0",
        "process": "^0.11.10",
        "stream-browserify": "^3.0.0",
        "stream-http": "^3.2.0",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.5.0",
        "ts-node": "^10.9.1",
        "twilio-run": "^3.5.4",
        "typescript": "^5.2.2",
        "url": "^0.11.3",
        "webpack": "^5.96.1",
        "webpack-cli": "^5.1.4"
    },
    "engines": {
        "node": "18"
    },
    "twilioserverless": {
        "runtime": "node18"
    }
}
